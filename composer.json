{
  "authors": [
    {
      "email": "tomaschochola@seznam.cz",
      "homepage": "https://github.com/tomaschochola",
      "name": "Tom치코 Chochola",
      "role": "Author"
    }
  ],
  "autoload": {
    "psr-4": {
      "Src\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "./tests"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    },
    "audit": {
      "abandoned": "fail"
    },
    "platform": {
      "php": "8.5.0"
    },
    "platform-check": true,
    "process-timeout": 0,
    "sort-packages": true
  },
  "description": "tomaschochola/php-template-mezzio",
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/tomaschochola"
    }
  ],
  "homepage": "https://github.com/tomaschochola/php-template-mezzio",
  "keywords": [
    "tomaschochola",
    "TomasChochola",
    "Tomas Chochola",
    "Tom치코 Chochola",
    "php-template-mezzio"
  ],
  "license": "CC-BY-ND-4.0",
  "minimum-stability": "dev",
  "name": "tomaschochola/php-template-mezzio",
  "prefer-stable": true,
  "readme": "./README.md",
  "repositories": [
    {
      "no-api": true,
      "type": "git",
      "url": "https://github.com/premierstacks/php-tooling-phpstan-config.git"
    },
    {
      "no-api": true,
      "type": "git",
      "url": "https://github.com/premierstacks/php-tooling-php-cs-fixer-config.git"
    }
  ],
  "require": {
    "ext-apcu": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "ext-redis": "*",
    "ext-zip": "*",
    "laminas/laminas-config-aggregator": "^1.19",
    "laminas/laminas-diactoros": "^3.8",
    "laminas/laminas-servicemanager": "^4.5",
    "mezzio/mezzio": "^3.27",
    "mezzio/mezzio-fastroute": "^3.14",
    "php": "^8.5.0"
  },
  "require-dev": {
    "ext-pcntl": "*",
    "ext-xdebug": "*",
    "phpunit/phpunit": "^12.5",
    "roave/security-advisories": "dev-latest",
    "tomaschochola/php-tooling-php-cs-fixer-config": "dev-main",
    "tomaschochola/php-tooling-phpstan-config": "dev-main"
  },
  "support": {
    "docs": "https://github.com/tomaschochola/php-template-mezzio",
    "email": "tomaschochola@seznam.cz",
    "issues": "https://github.com/tomaschochola/php-template-mezzio/issues",
    "source": "https://github.com/tomaschochola/php-template-mezzio.git"
  },
  "type": "project"
}
