{
  "name": "premierstacks/mezzio-api-template",
  "description": "premierstacks mezzio api template",
  "type": "project",
  "keywords": [
    "tomchochola",
    "tomaschochola",
    "Tomas Chochola",
    "Tom치코 Chochola",
    "premierstacks",
    "mezzio-api-template"
  ],
  "homepage": "https://github.com/premierstacks/mezzio-api-template",
  "readme": "./README.md",
  "license": "CC-BY-ND-4.0",
  "authors": [
    {
      "name": "Tom치코 Chochola",
      "email": "chocholatom1997@gmail.com",
      "homepage": "https://github.com/tomchochola",
      "role": "Author"
    }
  ],
  "support": {
    "email": "chocholatom1997@gmail.com",
    "issues": "https://github.com/premierstacks/mezzio-api-template/issues",
    "source": "https://github.com/premierstacks/mezzio-api-template.git",
    "docs": "https://github.com/premierstacks/mezzio-api-template"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/tomchochola"
    }
  ],
  "require": {
    "php": "~8.4.13",
    "composer/package-versions-deprecated": "^1.11.99.5",
    "laminas/laminas-component-installer": "^3.7.0",
    "laminas/laminas-config-aggregator": "^1.19.0",
    "laminas/laminas-diactoros": "^3.8.0",
    "laminas/laminas-stdlib": "^3.21.0",
    "mezzio/mezzio": "^3.23.2",
    "mezzio/mezzio-helpers": "^5.20.0",
    "laminas/laminas-servicemanager": "^3.24.0",
    "mezzio/mezzio-laminasrouter": "^3.12.0",
    "premierstacks/php-stack": "@dev"
  },
  "require-dev": {
    "laminas/laminas-development-mode": "^3.15.0",
    "mezzio/mezzio-tooling": "^2.12.0",
    "phpunit/phpunit": "^12.4.2",
    "roave/security-advisories": "@dev",
    "premierstacks/php-cs-fixer-stack": "@dev",
    "premierstacks/phpstan-stack": "@dev"
  },
  "autoload": {
    "psr-4": {
      "App\\": "./src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "./tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/premierstacks/php-cs-fixer-stack.git",
      "no-api": true
    },
    {
      "type": "vcs",
      "url": "https://github.com/premierstacks/phpstan-stack.git",
      "no-api": true
    },
    {
      "type": "vcs",
      "url": "https://github.com/premierstacks/php-stack.git",
      "no-api": true
    }
  ],
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "composer/package-versions-deprecated": true,
      "laminas/laminas-component-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "8.4.13"
    },
    "platform-check": true
  },
  "scripts": {
    "composer:audit": "@composer audit",
    "composer:install": "@composer install",
    "composer:platform": "@composer check-platform-reqs",
    "composer:update": "@composer update",
    "composer:validate": "@composer validate --strict --no-check-all",
    "dump:development": "@composer dump-autoload -o --dev --strict-psr",
    "dump:production": "@composer dump-autoload -a --no-dev --strict-psr",
    "fix:php-cs-fixer": "php-cs-fixer fix",
    "lint:php-cs-fixer": "php-cs-fixer fix --dry-run --diff",
    "stan:phpstan": "phpstan analyse",
    "start:development": [
      "Composer\\Config::disableProcessTimeout",
      "@putenv APP_ENV=local",
      "php -S 0.0.0.0:8080 -t ./public/"
    ],
    "start:coverage": [
      "Composer\\Config::disableProcessTimeout",
      "@putenv APP_ENV=local",
      "php -S 0.0.0.0:8080 -t ./.phpunit.coverage/html/"
    ],
    "test:phpunit": "phpunit",
    "post-install-cmd": "@config:clear",
    "post-update-cmd": "@config:clear",
    "development:disable": "laminas-development-mode disable",
    "development:enable": "laminas-development-mode enable",
    "development:status": "laminas-development-mode status",
    "mezzio": "laminas --ansi",
    "config:clear": "@php ./bin/config_clear.php"
  },
  "extra": {
    "laminas": {
      "component-whitelist": [
        "mezzio/mezzio",
        "mezzio/mezzio-helpers",
        "mezzio/mezzio-router",
        "laminas/laminas-httphandlerrunner",
        "mezzio/mezzio-laminasrouter"
      ]
    }
  }
}
